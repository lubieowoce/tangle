{
  "include": ["**/*.ts", "**/*.tsx", "**/*.jsx"],
  "compilerOptions": {
    "types": ["react/experimental", "react-dom/experimental"],
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": true,
    "noImplicitAny": false,
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["es2022", "DOM"],
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
