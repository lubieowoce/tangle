{
  "name": "@owoce/tangle",
  "version": "0.0.8",
  "description": "",
  "repository": {
    "url": "https://github.com/lubieowoce/tangle"
  },
  "module": "dist/runtime/index.js",
  "types": "dist/runtime/index.d.ts",
  "scripts": {
    "build": "npm run clean && run-p build:*",
    "build:bin": "tsup --config tsup.config.bin.js",
    "build:build": "tsc -p src/build/ && cp src/build/*.cjs dist/build/",
    "build:runtime": "tsc -p src/runtime/ && npm run postbuild:runtime",
    "postbuild:runtime": "echo '{ \"type\": \"module\" }' > dist/runtime/package.json",
    "watch": "npm run clean && run-p watch:*",
    "watch:runtime": "mkdir -p dist/runtime && npm run postbuild:runtime && tsc -p src/runtime/ --watch",
    "watch:bin": "npm run build:bin -- --watch",
    "clean": "rm -rf dist/*",
    "typecheck": "tsc -p src/runtime/ --noEmit",
    "lint": "eslint src/**"
  },
  "exports": {
    ".": {
      "react-server": "./dist/runtime/index.server.js",
      "node": "./dist/runtime/index.client.js",
      "default": "./dist/runtime/index.client.js"
    },
    "./client": {
      "types": "./dist/runtime/index.client.d.ts",
      "default": "./dist/runtime/index.client.js"
    },
    "./build": {
      "types": "./dist/build/index.d.ts",
      "default": "./dist/build/index.js"
    },
    "./build/babel-plugin-inline-actions": {
      "types": "./dist/build/babel-plugin-inline-actions.d.cts",
      "default": "./dist/build/babel-plugin-inline-actions.cjs"
    },
    "./shared": {
      "types": "./dist/runtime/index.shared.d.ts",
      "default": "./dist/runtime/index.shared.js"
    },
    "./server": {
      "types": "./dist/runtime/index.server.d.ts",
      "react-server": "./dist/runtime/index.server.js",
      "default": "./dist/runtime/server-only-error.js"
    }
  },
  "bin": {
    "tangle": "./dist/bin/tangle.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-syntax-jsx": "^7.22.5",
    "@owoce/react-server-dom-webpack": "^0.0.0-experimental-493f72b0a-20230727",
    "@owoce/tangle-router": "0.0.8",
    "babel-loader": "^9.1.3",
    "busboy": "^1.6.0",
    "chokidar": "^3.5.3",
    "css-loader": "^6.8.1",
    "enhanced-resolve": "^5.14.1",
    "express": "^4.18.2",
    "htmlescape": "^1.1.1",
    "micromatch": "^4.0.5",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.3",
    "react": "0.0.0-experimental-493f72b0a-20230727",
    "react-dom": "0.0.0-experimental-493f72b0a-20230727",
    "react-server-dom-webpack": "0.0.0-experimental-493f72b0a-20230727",
    "ts-loader": "^9.4.2",
    "typescript": "^5.1.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.9.0",
    "webpack-virtual-modules": "^0.5.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.3",
    "@types/busboy": "^1.5.0",
    "@types/express": "^4.17.17",
    "@types/htmlescape": "^1.1.1",
    "@types/micromatch": "^4.0.2",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.3",
    "tsup": "^6.6.3",
    "tsx": "^3.12.6",
    "vitest": "^0.34.6"
  }
}
