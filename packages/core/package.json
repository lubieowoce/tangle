{
  "name": "@owoce/tangle",
  "version": "0.0.4",
  "description": "",
  "repository": {
    "url": "https://github.com/lubieowoce/tangle"
  },
  "module": "dist/runtime/index.js",
  "types": "dist/runtime/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && npm run build:bin && npm run build:compiled",
    "build:bin": "tsup --config tsup.config.bin.js",
    "build:compiled": "tsc -p src/runtime/ && echo '{ \"type\": \"module\" }' > dist/runtime/package.json && cp -r src/types dist/",
    "typecheck": "tsc -p src/runtime/ --noEmit",
    "lint": "eslint src/**"
  },
  "exports": {
    ".": {
      "react-server": "./dist/runtime/index.server.js",
      "node": "./dist/runtime/index.client.js",
      "default": "./dist/runtime/index.client.js"
    },
    "./client": {
      "types": "./dist/runtime/index.client.d.ts",
      "default": "./dist/runtime/index.client.js"
    },
    "./server": {
      "types": "./dist/runtime/index.server.d.ts",
      "react-server": "./dist/runtime/index.server.js",
      "default": "./dist/runtime/server-only-error.js"
    }
  },
  "bin": {
    "tangle": "./dist/bin/tangle.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@owoce/tangle-router": "^0.0.1",
    "chokidar": "^3.5.3",
    "enhanced-resolve": "^5.14.1",
    "express": "^4.18.2",
    "micromatch": "^4.0.5",
    "react": "18.3.0-canary-910045696-20230607",
    "react-dom": "18.3.0-canary-910045696-20230607",
    "react-server-dom-webpack": "18.3.0-canary-910045696-20230607",
    "ts-loader": "^9.4.2",
    "typescript": "^5.1.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-virtual-modules": "^0.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/micromatch": "^4.0.2",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.3",
    "tsup": "^6.6.3",
    "tsx": "^3.12.6"
  }
}
